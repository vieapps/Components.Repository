<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<AllowUnsafeBlocks>false</AllowUnsafeBlocks>
		<RootNamespace>net.vieapps.Components.Repository</RootNamespace>
		<AssemblyName>VIEApps.Components.Repository</AssemblyName>
		<AssemblyTitle>VIEApps NGX Repository</AssemblyTitle>
		<AssemblyVersion>10.1.7.1802</AssemblyVersion>
		<FileVersion>10.1.7.1802</FileVersion>
		<InformationalVersion>v10.1.netstandard-2+rev:2018.01.23-sync-data-sources</InformationalVersion>
		<Product>VIEApps NGX</Product>
		<Company>VIEApps.net</Company>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<PackageId>VIEApps.Components.Repository</PackageId>
		<Version>10.1.7.1802</Version>
		<SignAssembly>false</SignAssembly>
		<DelaySign>false</DelaySign>
		<AssemblyOriginatorKeyFile>../VIEApps.Components.snk</AssemblyOriginatorKeyFile>
		<Title>VIEApps NGX Repository</Title>
		<Description>The tiny polyglot library to help .NET Standard 2.0 POCO objects work with both NoSQL and SQL databases in the same time just like other ORM-ish library does</Description>
		<Authors>VIEApps.net</Authors>
		<Copyright>Â© 2018 VIEApps.net</Copyright>
		<PackageTags>polyglot;repository;orm;activerecord;database;sql;nosql;vieapps;vieapps.components</PackageTags>
		<PackageProjectUrl>https://github.com/vieapps/Components.Repository</PackageProjectUrl>
		<RepositoryUrl>https://github.com/vieapps/Components.Repository</RepositoryUrl>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<WarningLevel>3</WarningLevel>
		<DocumentationFile>bin\Debug\netstandard2.0\VIEApps.Components.Repository.xml</DocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<WarningLevel>3</WarningLevel>
		<DocumentationFile>bin\Release\netstandard2.0\VIEApps.Components.Repository.xml</DocumentationFile>
		<DefineConstants>TRACE;RELEASE;NETSTANDARD2_0;PROCESSLOGS</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<None Remove=".DS_Store" />
		<None Remove=".gitattributes" />
		<None Remove=".gitignore" />
		<None Remove="LICENSE" />
		<None Remove="README.md" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MongoDB.Driver" Version="2.5.0" />
		<PackageReference Include="VIEApps.Components.Caching" Version="10.1.7.1802-beta" />
		<PackageReference Include="VIEApps.Components.Security" Version="10.1.7.1802-beta" />
		<PackageReference Include="VIEApps.Components.Utility" Version="10.1.7.1802-beta" />
	</ItemGroup>

</Project>
